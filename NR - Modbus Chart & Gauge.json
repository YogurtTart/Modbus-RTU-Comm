    {
        "id": "47365fee422a6091",
        "type": "function",
        "z": "f09e8f858d53ee98",
        "g": "1e087485c9719318",
        "name": "Node 1",
        "func": "// Get stored data\nlet nodesData = flow.get(\"nodesData\") || {};\n\n// Take the first node's data (if any)\nlet keys = Object.keys(nodesData);\nif (keys.length > 0) {\n    let data = nodesData[keys[0]];\n\n    let tempMsg = { payload: data.temperature, topic: \"temperature\" };\n    let humidMsg = { payload: data.humidity, topic: \"humidity\" };\n\n    return [null, null, tempMsg, humidMsg];\n} else {\n    return [null, null];\n}\n",
        "outputs": 4,
        "timeout": 0,
