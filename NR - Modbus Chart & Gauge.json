    {
        "id": "75cda6b92616f5a8",
        "type": "function",
        "z": "f09e8f858d53ee98",
        "g": "1e087485c9719318",
        "name": "function 3",
        "func": "// Get existing data or initialize\nlet nodesData = flow.get(\"nodesData\") || {};\n\n// Incoming message\nlet nodeID = msg.payload.nodeID;\n\n// Save/update the node's data (store entire payload)\nnodesData[nodeID] = { ...msg.payload };\n\n// Store back to flow context\nflow.set(\"nodesData\", nodesData);\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
